# Ignore bundler config.
rails-for-vue-ts-starter/.bundle

# Ignore all logfiles and tempfiles.
rails-for-vue-ts-starter/log/*
rails-for-vue-ts-starter/tmp/*
!rails-for-vue-ts-starter/log/.keep
!rails-for-vue-ts-starter/tmp/.keep
rails-for-vue-ts-starter/.byebug_history

# intellij folder ignore
.idea/*